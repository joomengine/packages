    "sql": "--\r\n-- Dumping data for table `#__[[[component]]]_address_type`\r\n--\r\n\r\nINSERT INTO `#__[[[component]]]_address_type` (`id`, `description`, `guid`, `name`, `params`, `published`, `created`, `modified`, `ordering`) VALUES\r\n(1, 'Primary on-site location where business operations or client services are physically conducted.', 'f1dadef2-f712-4e3f-b25e-5a7d05e784a3', 'Physical Address', '', 1, '2025-10-08 09:03:56', '2025-10-08 10:50:39', 1),\r\n(2, 'Main corporate or administrative headquarters used for official correspondence, registration, and central management.', 'e2f7c3c6-79c8-49a4-9e64-048b4d9020d7', 'Head Office', '', 1, '2025-03-28 09:00:00', '2025-03-28 09:00:00', 2),\r\n(3, 'Secondary office or regional branch supporting localized business operations and client engagement.', 'a1d4b9a9-b23d-4b4a-9133-5042c2b6df21', 'Branch Office', '', 1, '2025-03-28 09:00:00', '2025-03-28 09:00:00', 3),\r\n(4, 'Registered home or residential location of an individual service provider operating independently or remotely.', '5bb3d9e1-9e78-4b67-b1f1-c6c4e7d5174c', 'Residential Address', '', 1, '2025-03-28 09:00:00', '2025-10-10 13:50:58', 4),\r\n(5, 'Temporary project-based or client-site location used for on-site work, installations, or service delivery.', '2f3b5f6c-2bc9-4eb6-bda2-b915f11ac208', 'Project Site', '', 1, '2025-03-28 09:00:00', '2025-10-10 13:50:55', 5),\r\n(6, 'Dedicated facility used for inventory storage, logistics, and supply chain management operations.', 'f871c3e7-9a63-45e4-ae7c-89c76f3c01e4', 'Warehouse & Storage', '', 1, '2025-03-28 09:00:00', '2025-10-10 14:08:23', 6);\r\n\r\n",
